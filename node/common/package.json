{
  "name": "@eclipse-theiacloud/common",
  "version": "0.11.1",
  "description": "Common functionality for Theia Cloud",
  "license": "EPL-2.0",
  "keywords": [
    "theia",
    "cloud",
    "kubernetes",
    "k8s"
  ],
  "author": {
    "name": "Theia Cloud"
  },
  "homepage": "http://theia-cloud.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipsesource/theia-cloud.git"
  },
  "bugs": "https://github.com/eclipsesource/theia-cloud/issues",
  "contributors": [
    {
      "name": "Theia Cloud Project",
      "url": "http://theia-cloud.io"
    }
  ],
  "main": "lib/cjs/index.js",
  "types": "lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prepublishOnly": "npm run build",
    "publish:next": "npm pkg set version=`node --print \"require('./package.json').version\"`.`git rev-parse --short HEAD` && npm publish --tag next",
    "publish:latest": "npm publish --tag latest"
  },
  "dependencies": {
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "uuid": "^8.3.2"
  }
}
