{
  "name": "@eclipse-theiacloud/monitor-theia",
  "version": "0.9.0-next",
  "keywords": [
    "theia-extension"
  ],
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "@theia/core": "^1.34.0",
    "@theia/plugin-ext": "^1.34.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "publish:next": "npm version `npm pkg get version | tr -d '\"'`.`git rev-parse --short HEAD` && npm publish --tag next",
    "publish": "npm publish --tag latest"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/monitor-frontend-module",
      "backend": "lib/node/monitor-backend-module"
    }
  ]
}
