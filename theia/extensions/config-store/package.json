{
  "name": "@eclipse-theiacloud/config-store",
  "keywords": [
    "theia-extension"
  ],
  "version": "1.1.0-next",
  "author": {
    "name": "Theia Cloud"
  },
  "homepage": "http://theia-cloud.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse-theia/theia-cloud.git"
  },
  "bugs": "https://github.com/eclipse-theia/theia-cloud/issues",
  "contributors": [
    {
      "name": "Theia Cloud Project",
      "url": "http://theia-cloud.io"
    }
  ],
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/common/index.js",
  "typings": "lib/common/index.d.ts",
  "peerDependencies": {
    "@theia/core": "^1.55.0"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "~5.4.5"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint -c ../../.eslintrc.js --ext .ts ./src",
    "prepublishOnly": "npm run build",
    "publish:next": "npm pkg set version=`node --print \"require('./package.json').version\"`.`git rev-parse --short HEAD` && npm publish --tag next",
    "publish:latest": "npm publish --tag latest"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/config-store-frontend-module",
      "backend": "lib/node/config-store-backend-module"
    }
  ]
}
